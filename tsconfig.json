{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noEmit": true,
    "noEmitOnError": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": {
      "*": ["node_modules/*"],
      "@choiceform/ui-lottie/*": ["addon/*"],
      "@choiceform/ui-lottie/tests/*": ["tests/*"],
      "@choiceform/ui-lottie/config/*": ["config/*"],
      "dummy/*": ["tests/dummy/app/*", "app/*"],
      "dummy/tests/*": ["tests/*"],
      "@ember-decorators/*": ["node_modules/@ember-decorators/*"]
    },
    "pretty": true,
    "strictFunctionTypes": false,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "target": "es2017"
  },
  "include": [
    "addon",
    "app",
    "tests",
    "types",
  ]
}
